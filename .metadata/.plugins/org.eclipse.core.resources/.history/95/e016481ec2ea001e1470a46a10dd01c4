package Day_240325;

public class Barista {
	String name;
	int salary;
	int coffeeTotalNum;
	
	public Barista(String name) {
		this.name=name;
	}
	
	public void showInfo() {

		System.out.println("*** 바리스타 "+name+"이 만들었씁니다.");
	}
	
	public static void main(String[] args) {
		Barista baristalee = new Barista("lee");
		baristalee.showInfo();
	}
}
